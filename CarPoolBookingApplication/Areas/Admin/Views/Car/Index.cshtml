<div class="container p-4">
    <div class="row p-3">
        <div class="col-6">
            <h2 class="text-primary">List of Car</h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-primary" asp-action="createupdate"
               asp-controller="car">Create new Car</a>
        </div>
    </div>
</div>
<table id="myTable" class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Car Number
            </th>
            <th>
                Seat Capacity
            </th>
            <th>
                Action
            </th>
        </tr>
    </thead>
</table>
@section scripts
{
    <script src="~/js/Car.js"></script>
}